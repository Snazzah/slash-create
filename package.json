{
  "name": "slash-create",
  "version": "1.3.0",
  "description": "Creator and handler for Discord's slash commands",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "author": "Snazzah",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Snazzah/slash-create.git"
  },
  "bugs": {
    "url": "https://github.com/Snazzah/slash-create/issues"
  },
  "homepage": "https://slash-create.js.org/",
  "keywords": [
    "api",
    "discord",
    "discordapp",
    "slash-command",
    "slash-commands",
    "discord-interactions",
    "wrapper"
  ],
  "scripts": {
    "build": "tsc",
    "changelog": "ts-node scripts/changelog",
    "docs": "typedoc --json .tmp/typedoc-out.json --plugin typedoc-plugin-as-member-of && ts-node scripts/docs",
    "lint": "npx eslint --ext .ts ./src",
    "lint:fix": "npx eslint --ext .ts ./src --fix",
    "test": "mocha -r ts-node/register test/**/*.ts",
    "gpr": "ts-node scripts/gpr",
    "prepublishOnly": "npm run lint:fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run build && git add lib"
    }
  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "funding": {
    "url": "https://github.com/sponsors/Snazzah"
  },
  "dependencies": {
    "@discordjs/collection": "^0.1.6",
    "eventemitter3": "^4.0.7",
    "lodash": "^4.17.20",
    "require-all": "^3.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@sinonjs/fake-timers": "^7.0.1",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.14",
    "@types/require-all": "^3.0.1",
    "@types/sinonjs__fake-timers": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-nock": "^1.3.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "express": "^4.17.1",
    "fastify": "^3.9.1",
    "husky": "^4.3.6",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nock": "^13.0.5",
    "prettier": "^2.2.1",
    "ts-docgen": "github:dbots-pkg/ts-docgen#main",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typedoc-plugin-as-member-of": "^1.0.2",
    "typescript": "4.0"
  },
  "peerDependencies": {
    "express": "^4.17.1",
    "fastify": "^3.9.1"
  }
}
